<div class="table-responsive">
  <table class="table table-striped table-hover table-warehouse-items">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col" class="min-width-2">Producto</th>
        <th scope="col" class="min-width-6">Descripci√≥n</th>
        <th scope="col" class="min-width-1">C. Unidad</th>
        <th scope="col" class="min-width-1">C. Paq/Caja</th>
        <th scope="col" class="min-width-1">Subtotal</th>
      </tr>
    </thead>
    <tbody>
      <% item_transactions.each do |obj| %>
        <tr>
          <th scope="row"><%= obj.id %></th>
          <td class="min-width-2"><%= obj.item.product.name %></td>
          <td class="min-width-4"><%= obj.item.product.description %></td>
          <td class="min-width-1"><%= obj.qty %></td>
          <td class="min-width-1"><%= obj.qty / obj.item.product.total_unit%></td>
          <td class="min-width-1">
            <%= obj.sub_total %> <%= obj.transaktion.pos_session.type_of_currency %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
