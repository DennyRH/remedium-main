<div class="table-responsive">
  <table class="table table-striped table-hover table-warehouse-items">
    <thead>
      <tr>
        <th scope="col">Código</th>
        <th scope="col" class="min-width-4">Descripción</th>
        <th scope="col" class="min-width-3">Sucursal</th>
        <th scope="col" class="min-width-2">Proveedor</th>
        <th scope="col">Total stock</th>
        <th scope="col">Total ventas</th>
        <th scope="col">Total pedido</th>
        <th scope="col">Stock Min.</th>
        <th scope="col">Stock Max.</th>
      </tr>
    </thead>
    <tbody>
      <% items_suggestions.each do |obj| %>
        <tr>
          <td scope="row"><%= obj[:code] %></td>
          <td class="min-width-4"><%= obj[:description] %></td>
          <td class="min-width-2"><%= obj[:warehouse_name] %></td>
          <td><%= obj[:provider_name] %></td>
          <td><%= obj[:total_stock] %></td>
          <td><%= obj[:total_items_sold] %></td>
          <td><%= obj[:total_orders] %></td>
          <td><%= obj[:min_stock] %></td>
          <td><%= obj[:max_stock] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
