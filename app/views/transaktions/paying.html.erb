<%= turbo_frame_tag "modal_paying_transaktion" do %>
  <section id="myModal2" class="modal2">
    <div class="modal-content2">
      <h2>Pagar <%= @transaktion.type_transaktion_es %></h2>
      <div class="my-4">
        <%= render partial: "shared/forms/form_paying_transaktion",
            locals: {
              transaktion: @transaktion,
              pos_session: @pos_session,
              open_sessions: @open_sessions,
              payment_methods: @payment_methods
            }
        %>
      </div>
    </div>
  </section>
<% end %>
